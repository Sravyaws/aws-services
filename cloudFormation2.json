{
    "AWSTemplateFormatVersion": "2010-09-09",
    "Description": "This is my new vpc",
    "Resources": {
        "ntier": {
            "Type": "AWS::EC2::VPC",
            "Properties": {
                "Name": "ntier",
                "CidrBlock": "10.0.0.0/16",
                "EnableDnsHostnames": true,
                "Tags": [ 
                    {
                        "key": "Name",
                        "Value": "ntier"
                    },
                    {
                        "key": "Env",
                        "Value": "Dev"
                    }

                ]
            }
        }
    }
}
    