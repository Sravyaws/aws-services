{
    "AWSTemplateFormatVersion": "2010-09-09",
    "Description": "This is ntier vpc",
    "Parameters": {
        "vpccidr" : {
            "Type" : "String",
            "Default": "10.0.0.0/16",
            "Description": "vpc clidrblock"
          } 
    },
    "Resources": {
        "ntier": {
            "Type": "AWS::EC2::VPC",
            "Properties" : {
                "CidrBlock": {
                    "Ref": "vpccidr"
                },
                "EnableDnsHostnames": true,
                "Tags": [ 
                    {
                        "Key": "Name",
                        "Value": "ntier"
                    },
                    {
                        "Key": "Env",
                        "Value": "Dev"
                    },
                    {
                        "Key": "CreatedBy",
                        "Value": "Cloudformation"
                    }
                ]
            }
        }
    }
}
    
